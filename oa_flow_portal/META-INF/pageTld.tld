	<tag>
		<name>navigation</name>
		<tagclass>com.china.center.common.taglib.PageStart</tagclass>
		<bodycontent>JSP</bodycontent>
		<description>
		页面导航标签 (参数:height 设置导航栏的高度)
		</description>
		<attribute>
			<name>height</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>

	</tag>

