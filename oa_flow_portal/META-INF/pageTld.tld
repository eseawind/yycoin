	<tag>
		<name>link</name>
		<tagclass>com.china.center.common.taglib.PageLink</tagclass>
		<bodycontent>empty</bodycontent>
		<description>
		页面LINK标签,主要设置页面的样式和主题 &lt;br&gt;
		参数: &lt;br&gt;
		title:页面主题 (String) &lt;br&gt;
		cal:是否引用公共时间组件(boolean)&lt;br&gt;
		</description>
		<attribute>
			<name>cal</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>
			cal:是否引用公共时间组件(boolean)
			</description>
		</attribute>
		<attribute>
            <name>link</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <description>
            link:是否引用默认样式(boolean)
            </description>
        </attribute>
        <attribute>
            <name>guid</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <description>
            guid:是否引用flexigrid样式(boolean)
            </description>
        </attribute>
        <attribute>
            <name>dialog</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <description>
            dialog:是否引用dialog样式和脚本(boolean)
            </description>
        </attribute>
		<attribute>
			<name>title</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>

